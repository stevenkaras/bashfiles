#!/usr/bin/env bash

function test_patterns() {
    echo " 1#*x   = ${1#*x}"
    echo " 1#*x*  = ${1#*x*}"
    echo "  1#x*  = ${1#x*}"
    echo "1##*x   = ${1##*x}"
    echo "1##*x*  = ${1##*x*}"
    echo " 1##x*  = ${1##x*}"
    echo " 1%*x   = ${1%*x}"
    echo " 1%*x*  = ${1%*x*}"
    echo "  1%x*  = ${1%x*}"
    echo "1%%*x   = ${1%%*x}"
    echo "1%%*x*  = ${1%%*x*}"
    echo " 1%%x*  = ${1%%x*}"
}

for teststring in $@; do
    test_patterns $teststring
done
